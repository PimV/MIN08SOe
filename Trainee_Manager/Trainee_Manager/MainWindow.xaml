<Window x:Class="Trainee_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="900">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0">
            <Image Source="Images/AvansLogo.jpg" Margin="5,0"/>
        </Border>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0" Grid.Column="1" Grid.Row="1"/>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0" Grid.Row="1"/>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0" Grid.Column="1"/>
    </Grid>
</Window>
